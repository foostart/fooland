<% include header %>

<form action="/collectData" method="POST" class="col-md-4 offset-md-4">
    <div class="form-group">
        <label for="selSite">Chọn Site:</label>
        <select class="form-control" id="selSite">
            <% for(var i = 0; i < arraySite.length; i++){ %>
                <option value="<%= arraySite[i].site_id %>">
                    <%= arraySite[i].site_name %>
                </option>
                <% } %>
        </select>
    </div>

    <div class="form-group">
        <label for="pages">Nhập số trang cần lấy:</label>:</label>
        <input type="number" class="form-control" id="pages">
    </div>

    <button type="button" class="btn btn-success" id="btnSearch">Tìm kiếm</button>
</form>

<% include footer %>